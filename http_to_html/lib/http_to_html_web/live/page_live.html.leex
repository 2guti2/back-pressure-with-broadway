<div class="row">
  <div class="col col-md-12">
    <h1>Subscribers</h1>
  </div>
</div>
<div class="row">
  <%= for subscriber <- @subscribers do %>
   <div class="col col-md-4">
     <h3>Subscriber <%= subscriber.id %></h3>
     <ul>
       <%= for data <- subscriber.data do %>
        <li>
          <pre><%= inspect data %></pre>
        </li>
       <% end %>
     </ul>
   </div>
  <% end %>
</div>